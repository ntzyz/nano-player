    "build": "node node_modules/babel-cli/bin/babel src/player.js --out-file player.js",
    "compress": "node node_modules/uglify-js/bin/uglifyjs --output player.min.js player.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntzyz/nano-player.git"
  },
  "author": "ntzyz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntzyz/nano-player/issues"
  },
  "homepage": "https://github.com/ntzyz/nano-player#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "uglify-js": "^2.7.4"
  }
}
